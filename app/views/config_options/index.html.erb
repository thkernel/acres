<p id="notice"><%= notice %></p>

<h1>Config Options</h1>

<table>
  <thead>
    <tr>
      <th>App name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @config_options.each do |config_option| %>
      <tr>
        <td><%= config_option.app_name %></td>
        <td><%= link_to 'Show', config_option %></td>
        <td><%= link_to 'Edit', edit_config_option_path(config_option) %></td>
        <td><%= link_to 'Destroy', config_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Config Option', new_config_option_path %>
