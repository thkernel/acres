<div class="container">
	<div class="row">
		<div class="col-md-12">
			<div class="index-heading pull-left">
				<h3>Liste des injections</h3>
			</div>
			<div class="pull-right">
				<p>
				<%= link_to raw('<i class="fa fa-file-excel-o" aria-hidden="true"></i>
				Importer'), new_log_path, class: "btn btn-warning", remote: true %>
				</p>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">
				<div class="panel-body">
					<table id="logs-table-list" class="table table-striped table-bordered table-hover">
						<thead>
							<tr>
								<th >Date</th>
								<th>Fichier</th>
								<th>Nombre d'enregistrement</th>
								<th>Erreur</th>
								<th>Etat</th>
							</tr>
						</thead>
						<tbody class="imports">
							<%= render('index') %>
						</tbody>
					</table>
				</div>
			</div>
		</div>
		<div id="import-modal" class="modal fade"></div>
	</div>
</div>
