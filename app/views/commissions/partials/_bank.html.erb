

<div class="container">

<div class="row">
<div class="col-md-12">
<div class="index-heading pull-left">
<p>
    <h3>Résumé banque: <%= bank_name %></h3>
    </p>
    </div>
<div class="pull-right">

</div>
</div>
</div>

<div class="row">
<div class="col-md-12">
<div class="panel panel-default">
<div class="panel-body">
<table  id="table-list" class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>N°Dossier</th>
      <th>Date prod.</th>
      <th>Date acte</th>
      <th>Montant empruntés</th>
      <th>% com appo</th>
      <th>Com appo</th>
      <th>% com prod</th>
      <th>Com prod</th>
      <th>% com banque</th>
      <th>Com brute Acres</th>
      <th>% net Acres</th>
      <th>Com nette Acres</th>
    </tr>
  </thead>

  <tbody>
    <% @commissions.each do |commission| %>
      <tr>
        <td><%= commission.credit_id %></td>
        <td><%= commission.production_date %></td>
        <td><%= commission.acte_date %></td>
        <td><%= commission.amount_credit %></td>
        <td><%= commission.contributor_commission_percentage %></td>
        <td><%= commission.contributor_commission %></td>
        <td><%= commission.producer_commission_percentage %></td>
        <td><%= commission.producer_commission %></td>
        <td><%= commission.bank_commission_percentage %></td>
        <td><%= commission.bank_commission %></td>
        <td><%= "#{commission.company_commission_percentage}%" %></td>
        <td><%= commission.company_commission %></td>

        
      </tr>
    <% end %>
  </tbody>
</table>

</div>
</div>
</div>

</div>