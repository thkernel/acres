$("ul.errors").html("");
<% if @notary.errors.any? %>
  <% @notary.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".notaries").html("<%= escape_javascript(render 'index') %>");
  $("#notary-modal").modal("hide");
 
<% end %>