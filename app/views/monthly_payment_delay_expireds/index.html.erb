<p id="notice"><%= notice %></p>

<h1>Monthly Payment Delay Expireds</h1>

<table>
  <thead>
    <tr>
      <th>Credit identifier</th>
      <th>Expiration date</th>
      <th>Installment identifier</th>
      <th>Commission amount</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @monthly_payment_delay_expireds.each do |monthly_payment_delay_expired| %>
      <tr>
        <td><%= monthly_payment_delay_expired.credit_identifier %></td>
        <td><%= monthly_payment_delay_expired.expiration_date %></td>
        <td><%= monthly_payment_delay_expired.installment_identifier %></td>
        <td><%= monthly_payment_delay_expired.commission_amount %></td>
        <td><%= monthly_payment_delay_expired.user %></td>
        <td><%= link_to 'Show', monthly_payment_delay_expired %></td>
        <td><%= link_to 'Edit', edit_monthly_payment_delay_expired_path(monthly_payment_delay_expired) %></td>
        <td><%= link_to 'Destroy', monthly_payment_delay_expired, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Monthly Payment Delay Expired', new_monthly_payment_delay_expired_path %>
