
<div class="container">

<div class="row">
<div class="col-md-12">
<div class="index-heading pull-left">
    <h3>Historique des modifications de commissions banques</h3>
    </div>
<div class="pull-right">

</div>
</div>
</div>

<div class="row">
<div class="col-md-12">
<div class="panel panel-default">
<div class="panel-body">
<table  id="table-list" class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>Date modification</th>
      <th>Date d'effet</th>
      <th>Banque</th>
      <th>Nouvelle valeur</th>
      <th>Complet√©</th>
      
    </tr>
  </thead>

  <tbody>
    <% @bank_commission_editions.each do |bank_commission_edition| %>
      <tr>
		<td><%= bank_commission_edition.created_at %></td>
		<td><%= bank_commission_edition.created_at %></td>
		<td><%= bank_name(bank_commission_edition.bank_id) %></td>		
        <td><%= bank_commission_edition.new_value %></td>
        <td>
		
			<% if completed?(bank_commission_edition.completed) %>
      <span class="green-check"><i class="fa fa-check-circle" aria-hidden="true"></i></span>
      <% else %>
          <span class="import-failed"><i class="fa fa-check-circle" aria-hidden="true"></i></span>
            <% end %>

		</td>
       
      </tr>
    <% end %>
  </tbody>
  </table>
  </div>
  </div>
  </div>
  
  </div>
  
  <div id="bank-modal" class="modal fade"></div>